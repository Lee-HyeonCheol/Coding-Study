#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int N;
int main(void) {
	scanf("%d", &N);
	for (int i = 1; i <= N; i++) {
		for (int j = 0; j < i; j++)
			putchar('*');
		putchar('\n');
	}
	for (int i = N - 1; i >= 1; i--) {
		for (int j = 0; j < i; j++)
			putchar('*');
		putchar('\n');
	}
}